{
  "name": "cucumber-playwright",
  "version": "1.0.0",
  "description": "Starter project to write and debug cucumber-js features with Playwright",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "withoutreport": "cucumber-js src/features/**/*.feature --tags @RegressionTest22",
    "protocol": "cucumber-js src/features/**/*.feature --tags @ProtocolRunner",
    "test1": "cucumber-js src/features/**/*.feature --tags @RegressionTest  --format ./src/support/reporters/reporter.ts",
    "allure": "allure serve reports/allure-results"
  },
  "author": "poc-2-QA",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^8.9.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@types/node": "^20.4.9",
    "allure": "^0.0.0",
    "allure-cucumberjs": "^2.2.0",
    "aws": "^0.0.3-2",
    "aws-sdk": "^2.1364.0",
    "axios": "^1.3.6",
    "cucumber": "^6.0.7",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "playwright": "^1.36.2",
    "report": "^0.1.1",
    "reporter": "^0.1.0",
    "test": "^3.3.0",
    "ts-node": "^10.9.1",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^21.1.1",
    "allure-commandline": "^2.21.0"
  }
}